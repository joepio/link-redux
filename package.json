{
  "name": "link-redux",
  "version": "1.0.0-rc4",
  "description": "React/Redux extensions for the Link library",
  "main": "dist/link-redux.umd.js",
  "module": "dist/link-redux.es6.js",
  "sideEffects": false,
  "typings": "dist/typings/link-redux.d.ts",
  "files": [
    "@types",
    "dist/link-redux.es6.js",
    "dist/link-redux.umd.js",
    "dist/typings/**/*.*",
    "package.json",
    "tsconfig.json",
    "yarn.lock"
  ],
  "repository": "https://github.com/fletcher91/link-redux.git",
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig-types.json",
    "doc": "rm -rf dist/docs/* && typedoc --tsconfig tsconfig-types.json --exclude **/*.spec.ts --excludeExternals --excludePrivate --mode file --theme minimal --target ES6 --includeDeclarations --out dist/docs/ src/",
    "doc:deploy": "gh-pages -d ./dist/docs",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild": "rm -rf dist/*",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "author": "Fletcher91 <thom@argu.co>",
  "license": "LGPL-3.0",
  "peerDependencies": {
    "es6-promise": ">= 4.x",
    "hoist-non-react-statics": ">=3.x",
    "http-status-codes": ">= 1.x",
    "link-lib": ">=1.0.0-rc3",
    "prop-types": ">= 15.x",
    "rdflib": ">= 0.19.x",
    "react": ">= 16.10.x"
  },
  "devDependencies": {
    "@ontola/memoized-hash-factory": "^1.0.0",
    "@ontologies/core": "=1.5.2-strict-13",
    "@ontologies/rdf": "^1.0.0",
    "@ontologies/rdfs": "^1.0.0",
    "@ontologies/schema": "^1.0.0",
    "@ontologies/xsd": "^1.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.17",
    "@types/prop-types": "^15.7.1",
    "@types/react": "*",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "es6-promise": "4.x",
    "gh-pages": "^2.0.1",
    "hoist-non-react-statics": "^3.3.0",
    "http-status-codes": "1.x",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "link-lib": "=1.0.0-rc3",
    "prop-types": "^15.x",
    "rdflib": "^1.0.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "type-zoo": "^3.4.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.7.0-beta",
    "whatwg-fetch": "^3.0.0",
    "whatwg-url": "^7.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 95,
        "statements": 95
      }
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testMatch": [
      "**/*.spec.ts",
      "**/*.spec.tsx"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": ["./test/setup.ts"],
    "testURL": "http://example.org/resources/5",
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  }
}
